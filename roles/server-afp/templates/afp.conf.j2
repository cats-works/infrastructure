[Global]
mimic model = TimeCapsule6,106
{% if afp_listen_interface != "" %}
afp interfaces = {{ afp_listen_interface }}
{% endif %}

{% for share in afp_shares %}
[{{ share.name }}]
{% for key, value in share.items() if key != 'name' %}
   {{ key.replace('_', ' ') }} = {{ value }}
{% endfor %}

{% endfor %}
